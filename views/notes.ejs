<h1>Notes for Book</h1>
<% if (notes && notes.length > 0) { %>
    <% notes.forEach(note => { %>
        <p><%= note.notes %></p> <!-- Make sure you're using the correct field name from your database -->
    <% }) %>
    <% } else { %>
        <p>No notes available for this book.</p>
    <% } %>